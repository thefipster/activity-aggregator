{
  "Pipeline": {
    "IsSilent": false
  },
  "Stages": {
    "Importer": {
      "ImportDirectories": [
        "E:\\polar\\import\\polar_takeout",
        "E:\\polar\\import\\polar_protrainer",
        "E:\\polar\\import\\my_collection",
        "E:\\polar\\import\\fitbit_takeout\\FelixReisch\\Google Data\\Health Fitness Data",
        "E:\\polar\\import\\fitbit_takeout\\FelixReisch\\Google Data\\Physical Activity",
        "E:\\polar\\import\\google_timeline",
        "E:\\polar\\import\\thefipster_app"
      ],
      "MaxDegreeOfParallelism": 1
    },
    "Scanner": {
      "ExcludedFileExtensions": [".zip", ".png", ".xml", ".trk", ".txt", ".ovl", ".xls", ".kmz", ".jsp", "", ".dcm", ".crs", ".pdf", ".htm", ".html"],
      "MaxDegreeOfParallelism": 2
    },
    "Classifier": {
      "MaxDegreeOfParallelismDequeing": 2,
      "MaxDegreeOfParallelismClassifying": 1
    },
    "Extractor" : {
      "OutputDirectory": "E:\\polar\\pipeline\\extractor",
      "MaxDegreeOfParallelism": 16
    },
    "Bundler": {
      "ImportDirectory": "E:\\polar\\pipeline\\extractor",
      "MaxDegreeOfParallelism": 1
    },
    "Unifier": {
      "OutputDirectory": "E:\\polar\\pipeline\\unified",
      "MaxDegreeOfParallelism": 16
    }
  },
  "LiteDb": {
    "IndexFile": "E:\\polar\\pipeline\\db\\index.litedb",
    "ActivityFile": "E:\\polar\\pipeline\\db\\activity.litedb"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "File", "Args": { "path": "E:/polar/pipeline/logs/log-.txt", "rollingInterval": "Day" } }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "pipeline-cli"
    }
  }
}