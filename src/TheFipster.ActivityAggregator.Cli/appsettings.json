{
  "Pipeline": {
    "ImportDirectories": [
      "E:\\polar\\import\\polar_takeout",
      "E:\\polar\\import\\polar_protrainer",
      "E:\\polar\\import\\my_collection"
    ]
  },
  "Stages": {
    "Scanner": {
      "ExcludedFileExtensions": [".zip", ".png", ".xml", ".trk", ".txt", ".ovl", ".xls", ".kmz", ".jsp", "", ".dcm", ".crs", ".pdf", ".htm", ".html"],
      "MaxDegreeOfParallelism": 4
    },
    "Classifier": {
      "MaxDegreeOfParallelism": 4
    },
    "Extractor" : {
      "OutputDir": "E:\\polar\\pipeline\\extractor",
      "MaxDegreeOfParallelism": 8
    }
  },
  "LiteDb": {
    "IndexFile": "E:\\polar\\pipeline\\db\\index.litedb"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "File", "Args": { "path": "E:/polar/pipeline/logs/log-.txt", "rollingInterval": "Day" } }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "pipeline-cli"
    }
  }
}