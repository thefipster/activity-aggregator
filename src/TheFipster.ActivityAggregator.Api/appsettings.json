{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "OpenTelemetry"
      }
    ]
  },
  "LiteDb": {
    "IndexFile": "E:\\activity\\data\\db\\index.litedb",
    "ActivityFile": "E:\\activity\\data\\db\\activity.litedb"
  },
  "Api": {
    "Settings": {
      "MaxDegreeOfParallelism": 4
    },
    "Import": {
      "ImportFileExtensionFilter": ".zip",
      "UploadDirectoryPath": "E:\\activity\\data\\zips",
      "UnzipDirectoryPath": "E:\\activity\\data\\files",
      "ConvergeDirectoryPath": "E:\\activity\\data\\extracts",
      "MergeDirectoryPath": "E:\\activity\\data\\batch"
    }
  }
}
